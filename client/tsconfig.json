{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "moduleResolution": "node",
      "baseUrl": "src",
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      },
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "sourceMap": true,
      "strict": true
    },
    "include": ["src"]
  }